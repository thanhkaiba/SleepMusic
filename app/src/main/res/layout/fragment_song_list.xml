<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SongListFragment">


    <com.example.tienthanh.myapplication.MagicTextView
        android:id="@+id/category_name"
        xmlns:qwerjk="http://schemas.android.com/apk/res/com.example.tienthanh.myapplication"
        android:textSize="50sp"
        android:textColor="#C0C0C0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/sleep"
        android:textStyle="bold"
        android:padding="10dp"
        qwerjk:foreground="@drawable/fake_luxury_tiled"
        qwerjk:innerShadowDy="2dp"
        qwerjk:innerShadowColor="#FF000000"
        qwerjk:innerShadowRadius="1dp"
        qwerjk:outerShadowDy="3dp"
        qwerjk:outerShadowColor="#FF0088ff"
        qwerjk:outerShadowRadius="10dp"
        qwerjk:strokeColor ="#413d1a"
        qwerjk:strokeJoinStyle="miter"
        qwerjk:strokeWidth="5dp"
        android:text="Magic" />

    <ListView
        android:id="@+id/song_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </ListView>

</LinearLayout>